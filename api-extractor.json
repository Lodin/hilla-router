{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "projectFolder": ".",
  "mainEntryPointFilePath": "<projectFolder>/.temp/index.d.ts",
  "bundledPackages": [],
  "newlineKind": "lf",
  "testMode": true,
  "enumMemberOrder": "by-name",
  "compiler": {
    "tsconfigFilePath": "<projectFolder>/tsconfig.docs.json"
  },
  "apiReport": {
    "enabled": true,
    "reportFileName": "<unscopedPackageName>.api.md",
    "reportFolder": "<projectFolder>/.temp/",
    "reportTempFolder": "<projectFolder>/.temp/",
    "includeForgottenExports": false
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "<projectFolder>/.temp/<unscopedPackageName>.api.json",
    "includeForgottenExports": false,
    "projectFolderUrl": "https://github.com/ausginer/router"
  },
  "dtsRollup": {
    "enabled": true,
    "publicTrimmedFilePath": "<projectFolder>/index.d.ts"
  },
  "tsdocMetadata": {
    "enabled": true,
    "tsdocMetadataFilePath": "<projectFolder>/docs/tsdoc-metadata.json"
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": true
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": true
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": true
      }
    }
  }
}
