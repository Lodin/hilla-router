{
  "name": "@ausginer/router",
  "version": "0.1.0",
  "description": "A tiny frontend router for the frontend built on top of native URLPattern.",
  "main": "",
  "type": "module",
  "scripts": {
    "clean": "git clean -Xf . -e !.idea -e !.idea/**/* -e !.vscode -e !.vscode/**/* -e !node_modules -e !node_modules/**/* -n",
    "build": "concurrently npm:build:code npm:build:declaration",
    "build:code": "tsx scripts/build.ts",
    "build:declaration": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "test": "karma start karma.config.cjs",
    "test:watch": "npm run test -- --watch",
    "test:cover": "npm run test -- --coverage",
    "size": "size-limit"
  },
  "files": [
    "*.{d.ts.map,d.ts,js.map,js}"
  ],
  "repository": {
    "type": "git",
    "url": "https://github/ausginer/router"
  },
  "keywords": [
    "router",
    "universal",
    "routing",
    "routes",
    "route"
  ],
  "author": "Vlad Rindevich <vladrin@vaadin.com> (https://github.com/Lodin)",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./index.js": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./navigation.js": {
      "types": "./navigation.d.ts",
      "default": "./navigation.js"
    },
    "./Router.js": {
      "types": "./Router.d.ts",
      "default": "./Router.js"
    }
  },
  "overrides": {
    "esbuild": "$esbuild"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@size-limit/esbuild-why": "^8.2.4",
    "@size-limit/preset-small-lib": "^8.2.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/karma": "^6.3.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.2",
    "@types/sinon": "^10.0.14",
    "@types/sinon-chai": "^3.2.9",
    "chai-as-promised": "^7.1.1",
    "chai-dom": "^1.11.0",
    "concurrently": "^8.1.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.39.0",
    "eslint-config-vaadin": "^1.0.0-alpha.12",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-safarinative-launcher": "^1.1.0",
    "karma-spec-reporter": "^0.0.36",
    "karma-vite": "^1.0.4",
    "lint-staged": "^13.2.2",
    "micromatch": "^4.0.5",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "rollup": "^3.24.0",
    "simple-git-hooks": "^2.8.1",
    "sinon": "^15.0.4",
    "sinon-chai": "^3.7.0",
    "size-limit": "^8.2.4",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vite": "^4.3.3"
  },
  "dependencies": {
    "urlpattern-polyfill": "^8.0.2"
  },
  "size-limit": [
    {
      "path": [
        "Router.js",
        "navigation.js"
      ]
    }
  ]
}
