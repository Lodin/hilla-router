{
  "name": "@ausginer/router",
  "version": "1.0.0-beta.2",
  "description": "A tiny router for the frontend built on top of the native URLPattern.",
  "main": "",
  "type": "module",
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "git clean -Xf . -e !.idea -e !.idea/**/* -e !.vscode -e !.vscode/**/* -e !node_modules -e !node_modules/**/*",
    "check": "concurrently npm:check:*",
    "check:type": "tsc --noEmit",
    "check:lint": "eslint src/**/*.ts test/**/*.ts",
    "build": "concurrently npm:build:*",
    "build:code": "tsx scripts/build.ts",
    "build:declaration": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:copy-dts": "tsx scripts/copy-dts.ts",
    "docs": "typedoc",
    "test": "karma start karma.config.cjs",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "size": "npm run build && size-limit"
  },
  "files": [
    "*.{d.ts.map,d.ts,js.map,js}"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ausginer/router"
  },
  "keywords": [
    "router",
    "universal",
    "routing",
    "routes",
    "route",
    "framework-agnostic"
  ],
  "author": "Vlad Rindevich <vladrin@vaadin.com> (https://github.com/Lodin)",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./index.js": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
  },
  "overrides": {
    "esbuild": "$esbuild"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@microsoft/api-documenter": "^7.25.17",
    "@microsoft/api-extractor": "^7.47.9",
    "@mxssfd/typedoc-theme": "^1.1.7",
    "@size-limit/esbuild-why": "^11.1.6",
    "@size-limit/preset-small-lib": "^11.1.6",
    "@types/chai-as-promised": "^8.0.1",
    "@types/karma": "^6.3.8",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.7.4",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^4.0.0",
    "chai-as-promised": "^8.0.0",
    "chai-dom": "^1.12.0",
    "concurrently": "^9.0.1",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.0",
    "eslint-config-vaadin": "^1.0.0-alpha.28",
    "eslint-plugin-tsdoc": "^0.3.0",
    "glob": "^11.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-firefox-launcher": "^2.1.3",
    "karma-mocha": "^2.0.1",
    "karma-safarinative-launcher": "^1.1.0",
    "karma-spec-reporter": "^0.0.36",
    "karma-vite": "^1.0.5",
    "lint-staged": "^15.2.10",
    "micromatch": "^4.0.8",
    "mocha": "^10.7.3",
    "prettier": "^3.3.3",
    "simple-git-hooks": "^2.11.1",
    "sinon": "^19.0.2",
    "sinon-chai": "^4.0.0",
    "size-limit": "^11.1.6",
    "tsx": "^4.19.1",
    "type-fest": "^4.26.1",
    "typedoc": "^0.26.8",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "urlpattern-polyfill": "^10.0.0"
  }
}
